.head{
  background-color: #4789ed;
  width: 100%;
  height: 195px;
  overflow: hidden;
}
.head-con-top{
  width: 100%;
  margin-top: 30px;
  justify-content: center;
}
.headimg, .headimg image{
  width: 63px;
  height: 63px;
  border-radius: 50%;
}
.headimg{
  margin-right: 10px;
}

.mleft10{
  margin-left: 5px;
  margin-right: 5px;
}
.mtop5{
  margin-top: 5px;
}
.pright10{
 padding-left:10px; 
}
.member-order{
  margin-top: 20px;
  width: 100%;
  justify-content: center;
}
.grid{
  display: grid;
  grid-template-columns: 40% 40%;
  justify-content: center;
  justify-items:center;
  grid-column-gap: 30px;
  grid-row-gap: 20px;
  margin-top:15px;
}
.menu{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 85px;
  box-shadow: 0px 0px 10px 3px #efefef;
}
.menu text{
  margin-left: 10px;
  color: #000;
}
.menu-x{
  background: #999;
  box-shadow: 0px;
  color: #fff;
}
