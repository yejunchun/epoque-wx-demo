}
.pright10{
 padding-left:10px; 
}
.member-order{
  margin-top: 20px;
  width: 100%;
  justify-content: center;
}
.grid{
  display: grid;
  grid-template-columns: 120px 120px;
  grid-template-rows: 80px;
  justify-content: center;
  grid-column-gap: 30px;
  grid-row-gap: 20px;
  margin-top:15px;
}
.menu{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120px;
  height: 76px;
  box-shadow: 0px 0px 10px 3px #efefef;
}
.menu text{
  margin-left: 10px;
  color: #000;
}
.menu-x{
  background: #999;
  box-shadow: 0px;
  color: #fff;
}
.size32{
  color: #4789ed;
  font-size: 32px;
}