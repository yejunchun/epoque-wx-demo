.head{
  background-color: #4789ed;
  width: 100%;
  height: 195px;
  overflow: hidden;
}
.head-con-top{
  width: 100%;
  margin-top: 30px;
  justify-content: center;
}
.headimg, .headimg image{
  width: 63px;
  height: 63px;
  border-radius: 50%;
}
.headimg{
  margin-right: 10px;
}
