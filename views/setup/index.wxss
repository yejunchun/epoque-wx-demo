.set-content{
  position: absolute;
  width: 100%;
  height: 100%;
}
.set-list{
  height: 90%;
}
.setup{
  display: grid;
  grid-template-columns: 90% 10%;
  border-top:1px solid #e0e0e0;
  border-bottom:1px solid #e0e0e0;
  margin-top: 10px;
  padding-top: 5px;
  padding-bottom: 5px;
  font-size: 18px;
}
.setup text{
  padding-left: 5px;
}

.set-content .btn{
  background: #4f8dff;
  width: 95%;
  color: #fff;
}